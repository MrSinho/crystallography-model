PROJECT_NAME           = "crystallography model"
PROJECT_BRIEF          = by mrsinho.
PROJECT_NUMBER         = ${CRYSTAL_MODEL_VERSION}
PROJECT_LOGO           = 

USE_MDFILE_AS_MAINPAGE = ${CRYSTAL_MODEL_REPO_ROOT_DIR}/README.md

OUTPUT_DIRECTORY       = docs
INPUT                  = ${CRYSTAL_MODEL_REPO_ROOT_DIR}/crystal-model/include ${CRYSTAL_MODEL_REPO_ROOT_DIR}/README.md ${CRYSTAL_MODEL_REPO_ROOT_DIR}/.shci/linux/log.md ${CRYSTAL_MODEL_REPO_ROOT_DIR}/.shci/windows/log.md 
RECURSIVE              = YES
EXTRACT_ALL            = YES
HAVE_DOT               = NO
HIDE_UNDOC_MEMBERS     = YES
GENERATE_HTML          = YES
GENERATE_XML           = YES
GENERATE_LATEX         = YES
USE_MATHJAX            = YES
GENERATE_TREEVIEW      = YES
DISABLE_INDEX          = NO
FULL_SIDEBAR           = NO
OPTIMIZE_OUTPUT_FOR_C  = YES
STRIP_CODE_COMMENTS    = NO

IMAGE_PATH             = ${CRYSTAL_MODEL_REPO_ROOT_DIR}/docs/media ${CRYSTAL_MODEL_REPO_ROOT_DIR}/.shci/linux ${CRYSTAL_MODEL_REPO_ROOT_DIR}/.shci/windows

HTML_COLORSTYLE        = LIGHT
HTML_EXTRA_STYLESHEET  = ${CRYSTAL_MODEL_REPO_ROOT_DIR}/docs/doxygen-awesome-css/doxygen-awesome.css ${CRYSTAL_MODEL_REPO_ROOT_DIR}/docs/custom.css
HTML_EXTRA_FILES       = ${CRYSTAL_MODEL_REPO_ROOT_DIR}/docs/doxygen-awesome-css/doxygen-awesome-fragment-copy-button.js ${CRYSTAL_MODEL_REPO_ROOT_DIR}/docs/doxygen-awesome-css/doxygen-awesome-interactive-toc.js ${CRYSTAL_MODEL_REPO_ROOT_DIR}/docs/doxygen-awesome-css/doxygen-awesome-paragraph-link.js ${CRYSTAL_MODEL_REPO_ROOT_DIR}/docs/doxygen-awesome-css/doxygen-awesome-tabs.js ${CRYSTAL_MODEL_REPO_ROOT_DIR}/docs/doxygen-awesome-css/doxygen-awesome-darkmode-toggle.js

HTML_HEADER            = ${CRYSTAL_MODEL_REPO_ROOT_DIR}/docs/header.html

HTML_COLORSTYLE_HUE    = 209
HTML_COLORSTYLE_SAT    = 255
HTML_COLORSTYLE_GAMMA  = 113